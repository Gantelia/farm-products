{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "htmlacademy/react"
  ],
    "rules": {
    "react/prop-types": [
      "off"
    ]
  },
  "parser": "@babel/eslint-parser",
    "parserOptions": {
    "sourceType": "module",
      "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
