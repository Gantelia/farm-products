{
  "name": "farm-products",
  "version": "0.1.0",
  "private": "true",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.6",
    "swiper": "^9.0.5"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack server --mode development",
    "eslint": "eslint --ext .jsx --ext .js ./src/",
    "svgr": "svgr --ext .jsx -d ../farm-products/src/components/icons/ src/assets/svg/",
    "stylelint": "stylelint \"./src/**/*.{js,jsx}\"",
    "stylelint-check": "stylelint-config-prettier-check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "htmlacademy/react"
    ],
    "rules": {
      "react/prop-types": [
        "off"
      ]
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components",
      "stylelint-config-htmlacademy",
      "stylelint-config-prettier"
    ],
    "rules": {
      "selector-max-id": 1,
      "value-keyword-case": null,
      "no-empty-source": null
    },
    "customSyntax": "postcss-scss"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.3.1",
    "@svgr/webpack": "^6.3.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.3",
    "editorconfig": "^0.15.3",
    "eslint": "^8.24.0",
    "eslint-config-htmlacademy": "^7.0.0",
    "eslint-plugin-react": "^7.31.10",
    "html-webpack-plugin": "^5.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.8.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-htmlacademy": "^1.1.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
